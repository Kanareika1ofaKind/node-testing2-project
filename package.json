    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
